<div id="<%= dom_id song %>">
  <p>
    <strong>Title:</strong>
    <%= song.title %>
  </p>

  <p>
    <strong>Artist:</strong>
    <%= song.artist %>
  </p>

  <p>
    <strong>Upvotes:</strong>
    <%= song.upvotes %>
  </p>

  <p>
    <strong>Played:</strong>
    <%= song.played %>
  </p>

</div>
